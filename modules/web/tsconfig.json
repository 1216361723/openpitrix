{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2017",
      "es2020.promise",
      "es2021.String"
    ],
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
//    "isolatedModules": false,
    "noEmit": false
  },
  "include": [
    "./extensions",
    "extensions/app-store/src/types"
  ]
}
